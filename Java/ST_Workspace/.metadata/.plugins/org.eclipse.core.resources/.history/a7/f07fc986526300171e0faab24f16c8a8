package com.codingdojo.objectmaster;

public class HumanTest {

	public static void main(String[] args){
        Wizard clau  = new Wizard("Clau");
        Human adri = new Human("Adri");
        Wizard vale = new Wizard("Vale");
        clau.fireball(adri);
        adri.attack(clau);
        vale.heal(clau);

    }

}
